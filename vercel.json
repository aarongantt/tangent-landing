{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(iPhone|iPad|iPod|Android|webOS|BlackBerry|IEMobile|Opera Mini).*"
        }
      ],
      "destination": "/mobile.html",
      "permanent": false
    },
    {
      "source": "/demo.html",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/demo-tutorial.html",
      "destination": "/",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/styles.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
